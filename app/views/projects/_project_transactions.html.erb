		<ul>
			<% @project.transactions.each do |t| %>
				<li class="project-transaction project-li flaggable" flaggable_type='Transaction' flaggable_id=<%= t.id %>>
					<% if t.usd_defl.present? %>
						<b><%= "%.2f" % t.usd_defl %> <span class="usd-2009">USD-2009</span></b>
					<% end %>
					<% if !t.currency.nil? %>
					 (<%= t.value.blank? ? unset_data : "%.2f" % t.value.round(2) %> <%= t.currency.nil? ? '' : t.currency.iso3 %><% if t.usd_current && @project.year!=2009 && t.currency.iso3 != 'USD' %>,
					  	$<%= t.usd_current %><% end %><% if @project.year %> in <%= @project.year %><% end %>)
					<% end %>
					<% t.flags.each do |flag| %>
						<a inactive_href="#flag_<%= flag.id %>" class='flag-reference' style="color:<%= flag.flag_type.color %>;">[<%= @flag_counter +=1 %>] </a>
					<% end %>
				</li>
			<% end %>
		</ul>
