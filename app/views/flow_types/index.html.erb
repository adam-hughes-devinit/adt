<h1>Listing flow_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Iati code</th>
    <th>Aiddata code</th>
    <th>Oecd code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @flow_types.each do |flow_type| %>
  <tr>
    <td><%= flow_type.name %></td>
    <td><%= flow_type.iati_code %></td>
    <td><%= flow_type.aiddata_code %></td>
    <td><%= flow_type.oecd_code %></td>
    <td><%= link_to 'Show', flow_type %></td>
    <td><%= link_to 'Edit', edit_flow_type_path(flow_type) %></td>
    <td><%= link_to 'Destroy', flow_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

